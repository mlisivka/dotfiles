{
    "workbench.tree.indent": 15,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.colorCustomizations": {
        "tree.indentGuidesStroke": "#008070",
        "statusBar.background": "#005c20",
        "statusBar.noFolderBackground": "#005c20",
        "statusBar.debuggingBackground": "#005c20",
        "statusBar.foreground": "#ffffff",
        "statusBar.debuggingForeground": "#ffffff"
    },
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.colorTheme": "Monokai",
    "workbench.editor.enablePreview": false,
    "editor.cursorSurroundingLines": 8,
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "editor.stickyScroll.enabled": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.minimap.enabled": false,
    "editor.rename.enablePreview": false,
    "json.format.keepLines": true,
    "json.schemas": [
        {
            "fileMatch": [
                "*.json",
                ".prettierrc"
            ],
            "schema": {
                "allowTrailingCommas": true
            }
        }
    ],
    "files.associations": {
        ".prettierrc": "jsonc",
        "*.html.erb": "erb",
        "*.sql": "jinja-sql"
    },
    "prettier.prettierPath": "~/Documents/workspace/AndonPulse/andon_pulse/frontend/node_modules/prettier",
    "prettier.configPath": "~/Documents/workspace/AndonPulse/andon_pulse/frontend/.prettierrc",
    "rubyLsp.rubyVersionManager": {
        "identifier": "rbenv"
    },
    "rubyLsp.featureFlags": {
        "all": true
    },
    "[sql]": {
        "editor.defaultFormatter": "innoverio.vscode-dbt-power-user"
    },
    "[ruby]": {
        "editor.defaultFormatter": "Shopify.ruby-lsp",
        "editor.formatOnSave": false,
        "editor.formatOnType": false,
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "files.trimTrailingWhitespace": true,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true,
        "editor.rulers": [
            120
        ],
        "editor.semanticHighlighting.enabled": true
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.insertSpaces": false,
        "editor.tabSize": 2
    },
    "[erb]": {
        "editor.defaultFormatter": "aliariff.vscode-erb-beautify"
    },
    "emmet.includeLanguages": {
        "erb": "html",
        "ruby": "html"
    },
    "terminal.integrated.tabs.location": "left",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "extensions.ignoreRecommendations": true,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": "#005c20",
    "vim.statusBarColors.insert": "#000f5c",
    "vim.statusBarColors.visual": "#914a84",
    "vim.statusBarColors.visualline": "#914a84",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#007ACC",
    "vim.statusBarColors.searchinprogressmode": "#007ACC",
    "vim.statusBarColors.easymotionmode": "#007ACC",
    "vim.statusBarColors.easymotioninputmode": "#007ACC",
    "vim.statusBarColors.surroundinputmode": "#007ACC",
    "vim.leader": ",",
    "vim.easymotion": true,
    "vim.smartRelativeLine": true,
    "vim.hlsearch": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "#a9dc7660",
    "vim.highlightedyank.duration": 250,
    "vim.useSystemClipboard": true,
    "vim.foldfix": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "whichkey.show"
            ]
        },
        {
            "before": [
                "<space>"
            ],
            "after": [
                "a",
                " ",
                "<Esc>"
            ]
        },
        {
            "before": [
                "<leader>",
                "k"
            ],
            "after": [
                "O",
                "<Esc>"
            ],
            "silent": true
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "lineBreakInsert"
            ],
            "silent": true
        },
        {
            "before": [
                "g",
                "p",
                "d"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                "g",
                "h"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "g",
                "p",
                "i"
            ],
            "commands": [
                "editor.action.peekImplementation"
            ]
        },
        {
            "before": [
                "g",
                "q"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.referenceSearch.trigger"
            ]
        },
        {
            "before": [
                "g",
                "t"
            ],
            "commands": [
                "editor.action.goToTypeDefinition"
            ]
        },
        {
            "before": [
                "g",
                "p",
                "t"
            ],
            "commands": [
                "editor.action.peekTypeDefinition"
            ]
        },
        {
            "before": [
                "<C-a>"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "a",
                "i"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "r"
            ],
            "commands": [
                "find-it-faster.findWithinFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "g"
            ],
            "commands": [
                {
                    "command": "workbench.action.quickOpen",
                    "args": "%"
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "%"
            ],
            "commands": [
                "editor.emmet.action.matchTag"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "t"
            ],
            "commands": [
                "extension.runOpenSpec"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "l"
            ],
            "commands": [
                "extension.runLineOnRspec"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "f"
            ],
            "commands": [
                "extension.runFileOnRspec"
            ]
        }
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-]>"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "<C-e>"
            ],
            "after": [
                "<C-o>",
                "$"
            ]
        },
        {
            "before": [
                "<C-i>"
            ],
            "after": [
                "<C-o>",
                "I"
            ]
        },
        {
            "before": [
                "<leader>",
                "g"
            ],
            "after": [
                "<Alt-p>"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                "<leader>",
                "a",
                "i"
            ],
            "commands": [
                "editor.action.formatSelection"
            ]
        },
        {
            "before": [
                "<leader>",
                "%"
            ],
            "commands": [
                "editor.emmet.action.matchTag"
            ]
        },
        {
            "before": [
                "g",
                "q"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        }
    ],
    "javascript.updateImportsOnFileMove.enabled": "always",
    "search.exclude": {
        "**/sorbet": true
    },
    "sqlfluff.config": "${workspaceFolder}/../dbt_statistic/.sqlfluff",
    "sqlfluff.executablePath": "${workspaceFolder}/../dbt_statistic/dbt-env/bin/sqlfluff",
    "sqlfluff.linter.arguments": [],
    "sqlfluff.linter.run": "onType",
    "sqlfluff.linter.diagnosticSeverity": "error",
    "sqlfluff.linter.lintEntireProject": true,
    "sqlfluff.format.arguments": [
        "--FIX-EVEN-UNPARSABLE"
    ],
    "sqlfluff.format.enabled": true,
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "scminput",
        "clojure"
    ],
    "cursor.composer.shouldAllowCustomModes": true,
    "cursor.composer.shouldChimeAfterChatFinishes": true,
    "cursor.diffs.useCharacterLevelDiffs": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "find-it-faster.general.useTerminalInEditor": true,
    "cursor-retrieval.canAttemptGithubLogin": false,
    "calva.prettyPrintingOptions": {
        "printEngine": "pprint",
        "enabled": true,
        "width": 120,
        "maxLength": 50
    },
    "calva.paredit.defaultKeyMap": "strict",
    "tailwindCSS.includeLanguages": {
        "plaintext": "html"
    },
    "git.openRepositoryInParentFolders": "never",
    "cursor.general.globalCursorIgnoreList": [
        "**/.env",
        "**/.env.*",
        "**/credentials.json",
        "**/credentials.*.json",
        "**/secret.json",
        "**/secrets.json",
        "**/*.key",
        "**/*.pem",
        "**/*.pfx",
        "**/*.p12",
        "**/*.crt",
        "**/*.cer",
        "**/id_rsa",
        "**/id_dsa",
        "**/.ssh/id_*",
        "**/.kamal",
        "**/master.key"
    ],
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    }
}